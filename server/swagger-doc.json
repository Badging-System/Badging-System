{
  "info": {
    "title": "Badging System API Spec",
    "version": "1.0.0",
    "description": "API Specification for the Badging System API"
  },
  "host": "localhost:8080",
  "basePath": "/api/",
  "swagger": "2.0",
  "paths": {
    "/users": {
      "get": {
        "tags": [
          "User - Catalog"
        ],
        "description": "Gets of list of users",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "List of user data"
          }
        }
      }
    },
    "/users/{id}": {
      "get": {
        "tags": [
          "User - Catalog"
        ],
        "description": "Gets a specific user based off their unique id",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "User data"
          }
        }
      }
    },
    "/users/adduser": {
      "post": {
        "tags": [
          "User - Catalog"
        ],
        "description": "Inserts one new specified user",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/username"
          },
          {
            "$ref": "#/components/parameters/firstname"
          },
          {
            "$ref": "#/components/parameters/lastname"
          },
          {
            "$ref": "#/components/parameters/status"
          },
          {
            "$ref": "#/components/parameters/email"
          }
        ],
        "responses": {
          "201": {
            "description": "New user has been entered"
          }
        }
      }
    },
    "/users/addedusers": {
      "post": {
        "tags": [
          "User - Catalog"
        ],
        "description": "Inserts multiple new specified users",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/username"
          },
          {
            "$ref": "#/components/parameters/firstname"
          },
          {
            "$ref": "#/components/parameters/lastname"
          },
          {
            "$ref": "#/components/parameters/status"
          },
          {
            "$ref": "#/components/parameters/email"
          }
        ],
        "responses": {
          "201": {
            "description": "Multiple new users have been entered"
          }
        }
      }
    }
  },
  "definitions": {},
  "responses": {},
  "parameters": {},
  "securityDefinitions": {},
  "tags": [],
  "components": {
    "parameters": {
      "username": {
        "in": "formData",
        "name": "Username",
        "required": true,
        "schema": {
          "type": "string",
          "format": "byte"
        }
      },
      "firstname": {
        "in": "formData",
        "name": "First_name",
        "required": true,
        "schema": {
          "type": "string",
          "format": "byte"
        }
      },
      "lastname": {
        "in": "formData",
        "name": "Last_name",
        "required": true,
        "schema": {
          "type": "string",
          "format": "byte"
        }
      },
      "status": {
        "in": "formData",
        "name": "Status",
        "required": true,
        "schema": {
          "type": "string",
          "format": "byte"
        }
      },
      "email": {
        "in": "formData",
        "name": "Email",
        "required": true,
        "schema": {
          "type": "string",
          "format": "email"
        }
      }
    }
  }
}